{
  "metadata": {
    "name": "Gaia taskgraph",
    "description": "Gaia taskgraph generator"
  },
  "scopes": [
    "docker-worker:cache:tc-vcs",
    "docker-worker:cache:gaia"
  ],
  "tags": {},
  "tasks": [
    {
      "reruns": 3,
      "task": {
        "metadata": {
          "name": "Gaia Taskcluster Decision Task"
        },
        "provisionerId": "aws-provisioner",
        "workerType": "gaia-decision",
        "scopes": ["docker-worker:cache:tc-vcs", "docker-worker:cache:gaia"],
        "payload": {
          "cache": {
            "tc-vcs": "/home/tester/.tc-vcs/",
            "gaia": "/home/tester/workspace/"
          },
          "image": "taskcluster/gaia-taskenv:0.8.1",
          "command": [
            "entrypoint",
            "echo 'what' && make node_modules && node tests/taskcluster/bin/graph > /graph.json && cat /graph.json"
          ],
          "graphs": ["/graph.json"]
        },
        "extra": {
          "treeherder": {
            "symbol": "D",
            "groupSymbol": "?"
          }
        }
      }
    }
  ]
}
