{
  "search_providers": {

    "yahoo": {
      "title": "Yahoo",
      "search": {
        "url": "https://search.yahoo.com/yhs/search",
        "params": {"p": "{searchTerms}"}
      },
      "suggest": {
        "url": "https://search.yahoo.com/sugg/ff",
        "params": {"command": "{searchTerms}", "output": "fxjson"}
      }
    },

    "ddg": {
      "title": "DuckDuckGo",
      "search": {
        "url": "https://duckduckgo.com/",
        "params": {"t": "ffos", "q": "{searchTerms}"}
      },
      "suggest": {
        "url": "https://ac.duckduckgo.com/ac/",
        "params": {"q": "{searchTerms}", "type": "list"}
      }
    },

    "google": {
      "title": "Google",
      "search": {
        "url": "https://www.google.com/search",
        "params": {"q": "{searchTerms}"}
      },
      "suggest": {
        "url": "https://www.google.com/complete/search",
        "params": {"q": "{searchTerms}", "client": "firefox"}
      }
    },

    "bing": {
      "title": "Bing",
      "search": {
        "url": "https://www.bing.com/search",
        "params": {"q": "{searchTerms}"}
      },
      "suggest": {
        "url": "https://www.bing.com/osjson.aspx",
        "params": {"query": "{searchTerms}"}
      }
    }

  },

  "simConfigs": {
  },

  "partnerConfig": {
  },

  "defaultEngines": {
    "defaultEngine": "google",
    "providers": {
      "yahoo": {},
      "google": {},
      "bing": {},
      "ddg": {}
    }
  },

  "locales": {
  }
}
