{
  "name": "Sync",
  "description": "Syncs data with Firefox Sync",
  "type": "certified",
  "launch_path": "/index.html",
  "developer": {
     "name": "The Gaia Team",
     "url": "https://github.com/mozilla-b2g/gaia"
  },
  "datastores-owned": {
    "firefox-sync-bookmarks": {
      "access": "readwrite",
      "description": "View and modify the bookmarks from Firefox Sync."
    }
  },
  "datastores-access": {
    "places": {
      "readonly": false,
      "description": "Stores data about browsing history."
    },
    "bookmarks_store": {
      "readonly": false,
      "description": "View and modify bookmarks"
    }
  },
  "permissions": {
    "moz-firefox-accounts": {}
  }
}
