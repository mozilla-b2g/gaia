{
  "name": "Share Activity",
  "description": "An app which displays the details of a received share activity.",
  "type": "certified",
  "launch_path": "/index.html",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "activities": {
    "share": {
      "filters": {
        "type": ["audio/*", "image/*", "url",
                 "video/*", "application/*", "text/vcard"]
      },
      "disposition": "window",
      "returnValue": true
    }
  }
}
