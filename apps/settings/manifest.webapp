      "description": "Gaia Einstellungen"
    },
    "el": {
      "name": "Ρυθμίσεις",
      "description": "Ρυθμίσεις Gaia"
    },
    "en-US": {
      "name": "Settings",
      "description": "Gaia Settings"
    },
    "es": {
      "name": "Configuración",
      "description": "Configuración de Gaia"
    },
    "fr": {
      "name": "Paramètres",
      "description": "Paramètres Gaia"
    },
    "nb-NO": {
