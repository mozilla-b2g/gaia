{
  "name": "Vertical",
  "description": "A vertical homescreen for FirefoxOS.",
  "launch_path": "/index.html",
  "type": "certified",
  "role": "homescreen",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": {
    "mobileconnection":{},
    "open-remote-window":{},
    "storage": {},
    "webapps-manage": {},
    "systemXHR": {},
    "settings":{ "access": "readwrite" }
  },
  "default_locale": "en-US",
  "icons": {
    "30": "/style/icons/vertical_30.png",
    "45": "/style/icons/vertical_45.png",
    "60": "/style/icons/vertical_60.png",
    "68": "/style/icons/vertical_68.png"
  },
  "orientation": "default",
  "activities": {},
  "messages": [],
  "datastores-access": {
    "bookmarks_store": {
      "readonly": true,
      "description": "Display bookmarks"
    },
    "collections_store": {
      "readonly": true,
      "description": "Display collections"
    }
  },
  "datastores-owned": {
    "vertical_preferences_store": {
      "access": "readwrite",
      "description": "Stores preferences"
    }
  }
}
