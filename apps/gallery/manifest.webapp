{
  "activities": {
    "browse": {
      "disposition": "window", 
      "filters": {
        "type": "photos"
      }
    }, 
    "open": {
      "href": "/open.html", 
      "returnValue": true, 
      "disposition": "inline", 
      "filters": {
        "type": [
          "image/jpeg", 
          "image/png"
        ]
      }
    }, 
    "pick": {
      "href": "/index.html#pick", 
      "returnValue": true, 
      "disposition": "inline", 
      "filters": {
        "type": [
          "image/jpeg", 
          "image/png"
        ]
      }
    }
  }, 
  "name": "Gallery", 
  "default_locale": "en-US", 
  "icons": {
    "120": "/style/icons/Gallery.png", 
    "60": "/style/icons/60/Gallery.png"
  }, 
  "description": "Gaia Gallery", 
  "launch_path": "/index.html", 
  "permissions": {
    "device-storage:videos": {
      "access": "readwrite"
    }, 
    "device-storage:pictures": {
      "access": "readwrite"
    }, 
    "settings": {
      "access": "readonly"
    }
  }, 
  "type": "certified", 
  "locales": {
    "zh-TW": {
      "name": "\u76f8\u7c3f", 
      "description": "Gaia \u76f8\u7c3f"
    }, 
    "en-US": {
      "name": "Gallery", 
      "description": "Gaia Gallery"
    }, 
    "ar": {
      "name": "\u0625\u0633\u062a\u062f\u064a\u0648", 
      "description": "Gaia \u0625\u0633\u062a\u062f\u064a\u0648"
    }, 
    "fr": {
      "name": "Galerie", 
      "description": "Galerie Gaia"
    }
  }, 
  "developer": {
    "url": "https://github.com/mozilla-b2g/gaia", 
    "name": "The Gaia Team"
  }
}