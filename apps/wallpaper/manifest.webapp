{
  "name": "Wallpaper",
  "description": "Gaia Wallpaper Picker",
  "type": "certified",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": {
    "settings":{}
  },
  "activities": {
    "pick": {
      "filters": {
        "type": ["image/jpeg", "image/png"],
        "width": 320,
        "height": 480
       },
      "disposition": "inline",
      "returnValue": true,
      "href": "/pick.html"
    },
    "share": {
      "filters": {
        "type": "image/*",
        "number": 1
       },
      "disposition": "inline",
      "returnValue": true,
      "href": "/share.html"
    }
  }
}
