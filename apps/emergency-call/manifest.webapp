{
  "name": "EmergencyCall",
  "description": "The emergency call pad",
  "type": "certified",
  "role": "system",
  "launch_path": "/index.html",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "orientation": "default",
  "fullscreen": true,
  "permissions": {
    "themeable":{},
    "telephony": {},
    "mobileconnection": {},
    "phonenumberservice": {},
    "settings": { "access": "readonly" },
    "contacts": { "access": "readonly" }
  },
  "datastores-access": {
    "ice_contacts": {
      "readonly": true,
      "description": "Contains references to in case of emergency contacts"
    }
  },
  "locales": {
    "en-US": {
      "name": "EmergencyCall",
      "description": "Web app template"
    }
  },
  "default_locale": "en-US",
  "icons": {
    "84": "/style/icons/emergency_call_84.png",
    "126": "/style/icons/emergency_call_126.png",
    "142": "/style/icons/emergency_call_142.png",
    "189": "/style/icons/emergency_call_189.png",
    "284": "/style/icons/emergency_call_284.png"
  }
}
