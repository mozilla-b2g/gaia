{
  "name": "Gaia Test Keyboard",
  "description": "for Gaia 3rd-party keyboard testing",
  "type": "certified",
  "role": "keyboard",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": {
    "keyboard": {
      "description": "To send key events"
    }
  },
  "entry_points": {
    "english": {
      "launch_path": "/index.html#en",
      "name": "English",
      "description": "English layout",
      "types": ["text", "url", "number"]
    }
  },
  "icons": {
    "128": "/style/icons/test-keyboard.png"
  },
  "locales": {
    "en-US": {
      "name": "Gaia Test Keyboard",
      "description": "for Gaia 3rd-party keyboard testing"
    },
    "zh-TW": {
      "name": "Gaia 測試鍵盤",
      "description": "for Gaia 3rd-party keyboard testing"
    }
  },
  "default_locale": "en-US",
  "activities": {
    "keyboard-settings": {
      "href": "./settings.html",
      "disposition": "inline",
      "filters": {
        "type": "Gaia Test Keyboard"
      },
      "returnValue": true
    }
  }
}
