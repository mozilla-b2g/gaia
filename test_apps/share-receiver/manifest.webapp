{
  "name": "Share Receiver",
  "description": "Dummy app to respond to Share web activities",
  "launch_path": "/index.html",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": [
    "device-storage"
  ],
  "activities": {
    "share-filenames": {
      "filters": {
        "type": "image"
       },
      "disposition": "window"
    }
  },
  "icons": {
    "128": "/style/icons/Blank.png"
  }
}
