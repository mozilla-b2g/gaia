{
  "name": "Image Uploader",
  "description": "Uploading an image to some website",
  "launch_path": "/index.html",
  "developer": {
    "name": "lissyx",
    "url": "https://github.com/lissyx/gaia/tree/image-uploader"
  },
  "permissions": [
    "systemXHR",
    "device-storage"
  ],
  "activities": {
    "share-filenames": {
      "filters": {
        "type": "image"
       },
      "disposition": "window"
    }
  },
  "icons": {
    "128": "/style/icons/share.png"
  },
  "hidden": "hidden"
}
