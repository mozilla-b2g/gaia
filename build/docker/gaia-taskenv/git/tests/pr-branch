#! /bin/bash -vex

source setup.sh

git-ci-checkout-pr \
  $STAGE_DIR \
  https://github.com/lightsofapollo/github-workflow-test.git \
  branch \
  branch \
  https://github.com/lightsofapollo/github-workflow-test.git \
  pr \
  6c90f4ce3878d7c9ebf1280bc614f8d472ba1ba4

# We are targeting branch
test -f $STAGE_DIR/branch
test -f $STAGE_DIR/pr
# But have commits from master too.
test -f $STAGE_DIR/master


