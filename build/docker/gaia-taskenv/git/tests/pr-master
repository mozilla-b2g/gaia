#! /bin/bash -vex

source setup.sh

test_commands() {
  git-ci-checkout-pr \
    $STAGE_DIR \
    https://github.com/lightsofapollo/github-workflow-test.git \
    master \
    https://github.com/lightsofapollo/github-workflow-test.git \
    pr

  test -f $STAGE_DIR/master
  test -f $STAGE_DIR/pr
}

test_commands
test_commands
