#! /bin/bash -vex

source setup.sh

test_commands() {
  git-ci-checkout-pr \
    $STAGE_DIR \
    https://github.com/lightsofapollo/github-workflow-test.git \
    master \
    master \
    https://github.com/lightsofapollo/github-workflow-test.git \
    pr \
    6c90f4ce3878d7c9ebf1280bc614f8d472ba1ba4

  test -f $STAGE_DIR/master
  test -f $STAGE_DIR/pr
}

# Ensure we are idempotent
test_commands
test_commands

