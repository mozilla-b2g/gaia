#! /bin/bash -vex

source setup.sh

test_commands() {
  git ci-checkout-ref \
    $STAGE_DIR \
    https://github.com/lightsofapollo/github-workflow-test.git \
    branch \
    f6136f6e4eeaef7c0d26d061acdb1cda9c07d1d6

  test -f $STAGE_DIR/branch
  test ! -f $STAGE_DIR/master
}


# Run it twice to ensure its repeatable.
test_commands
test_commands
