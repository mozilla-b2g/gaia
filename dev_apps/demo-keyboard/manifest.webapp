{
  "name": "Demo Keyboard",
  "description": "for Gaia 3rd-party keyboard testing",
  "type": "certified",
  "role": "input",
  "launch_path": "/settings_panel.html",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": {
    "settings": { "access": "readwrite" },
    "input": {
      "description": "To send key events"
    }
  },
  "locales": {
    "ar": {
      "name": "Demo Keyboard",
      "description": "for Gaia 3rd-party keyboard testing"
    },
    "en-US": {
      "name": "Demo Keyboard",
      "description": "for Gaia 3rd-party keyboard testing"
    },
    "fr": {
      "name": "Demo Keyboard",
      "description": "for Gaia 3rd-party keyboard testing"
    },
    "zh-TW": {
      "name": "Demo Keyboard",
      "description": "for Gaia 3rd-party keyboard testing"
    }
  },
  "default_locale": "en-US",
  "inputs": {
    "en": {
      "launch_path": "/index.html#en",
      "name": "English",
      "description": "English layout",
      "types": ["text", "url", "email"]
    },
    "es": {
      "launch_path": "/index.html#es",
      "name": "Spanish",
      "description": "Spanish layout",
      "types": ["text", "url", "email"]
    }
  }
}
