{
  "version": "1.0.0",
  "name": "Mobile Wallet",
  "description": "New wallet version for FFOS 2.2",
  "launch_path": "/index.html",
  "icons": {
    "84": "/img/icons/default_84.png",
    "126": "/img/icons/default_126.png",
    "142": "/img/icons/default_142.png",
    "189": "/img/icons/default_189.png",
    "284": "/img/icons/default_284.png"
  },
  "type": "certified",
  "permissions": {
    "secureelement-manage": {},
    "nfc-hci-events":{}
  },
  "secure_element_access": ["*/A0*"],
  "messages": [
     { "nfc-hci-event-transaction": "/index.html" }
  ],
  "orientation": "portrait",
  "default_locale": "en"
}