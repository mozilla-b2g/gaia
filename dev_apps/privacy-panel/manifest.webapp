{
  "name": "Privacy Panel",
  "description": "Web app Privacy Panel",
  "type": "certified",
  "launch_path": "/index.html",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "permissions": {
    "audio-channel-alarm":{},
    "audio-channel-notification":{},
    "bluetooth":{},
    "browser": {},
    "desktop-notification":{},
    "device-storage:pictures":{ "access": "readonly" },
    "device-storage:music":{ "access": "readonly" },
    "device-storage:videos":{ "access": "readonly" },
    "device-storage:sdcard":{ "access": "readwrite" },
    "device-storage:apps":{ "access": "readonly" },
    "downloads":{},
    "embed-apps": {},
    "firefox-accounts": {},
    "idle":{},
    "mobileconnection":{},
    "nfc":{ "access": "readonly" },
    "open-remote-window":{},
    "permissions":{},
    "power":{},
    "settings":{ "access": "readwrite" },
    "sms":{},
    "storage":{},
    "systemXHR": {},
    "telephony":{},
    "time":{},
    "voicemail":{},
    "webapps-manage":{},
    "wifi-manage":{}
  },
  "activities": {
    "configure": {
      "filters": {
        "target": "device"
       },
      "disposition": "window"
    }
  },
  "connections": {
    "privacy-panel-wakeup": {
      "rules": {
        "manifestURLs": [
          "app://system.gaiamobile.org/manifest.webapp"
        ]
      }
    }
  },
  "datastores-access": {
    "download_store": {
      "readonly": false,
      "description": "Stores download finished"
    },
    "vertical_preferences_store": {
      "readonly": false,
      "description": "Vertical homescreen preferences"
    }
  },
  "locales": {
    "ar": {
      "name": "Privacy Panel",
      "description": "Web app Privacy Panel"
    },
    "de": {
      "name": "Privacy Panel",
      "description": "Web app Privacy Panel"
    },
    "el": {
      "name": "Privacy Panel",
      "description": "Web app Privacy Panel"
    },
    "en-US": {
      "name": "Privacy Panel",
      "description": "Web app Privacy Panel"
    },
