    },
    "fr": {
      "name": "Privacy Panel",
      "description": "Web app Privacy Panel"
    },
    "it": {
      "name": "Privacy Panel",
      "description": "Web app Privacy Panel"
    },
    "pt-BR": {
      "name": "Privacy Panel",
