homepage                        : 'http://github.com/dsc/node-remove'
keywords                        : <[ util server files fs rm remove delete recursive ]>
author                          : 'David Schoonover <dsc@less.ly> (http://less.ly)'

# main                            : './remove'
# directories                   : lib: './lib', bin:'./bin', doc:'./doc', man:'./man'

